# VeilSupport Environment Configuration
# Copy this file to .env and update with your values

# Server Configuration
VEILSUPPORT_SERVER_PORT=8080
VEILSUPPORT_SERVER_HOST=localhost
VEILSUPPORT_SERVER_TOR_PROXY=socks5://127.0.0.1:9050

# XMPP Configuration
VEILSUPPORT_XMPP_SERVER=localhost:5222
VEILSUPPORT_XMPP_ADMIN=admin@localhost
VEILSUPPORT_XMPP_PASSWORD=admin_password_here
VEILSUPPORT_XMPP_DOMAIN=localhost

# Database Configuration
VEILSUPPORT_DATABASE_HOST=localhost
VEILSUPPORT_DATABASE_PORT=5432
VEILSUPPORT_DATABASE_USER=veilsupport
VEILSUPPORT_DATABASE_PASSWORD=your_db_password_here
VEILSUPPORT_DATABASE_DB_NAME=veilsupport
VEILSUPPORT_DATABASE_SSL_MODE=disable

# JWT Configuration
VEILSUPPORT_JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_random
VEILSUPPORT_JWT_TTL=3600

# Redis Configuration  
VEILSUPPORT_REDIS_URL=redis://localhost:6379